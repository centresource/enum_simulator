ActiveRecord::Schema.define(:version => 0) do
  create_table :thingies, :force => true do |t|
    t.string :flavor, :null => false
    t.string :smell
    t.timestamps
  end
  create_table :other_thingies, :force => true do |t|
    t.string :taste
    t.timestamps
  end
  create_table :foos, :force => true do |t|
    t.string :first_enum
    t.string :second_enum
    t.timestamps
  end
end
